{
    "name": "@blocz/mdx-live",
    "version": "0.3.0",
    "type": "module",
    "source": "src/index.ts",
    "main": "lib/mdx-live.js",
    "module": "lib/mdx-live.js",
    "types": "lib/index.d.ts",
    "keywords": [
        "mdx",
        "markdown",
        "jsx",
        "remark",
        "mdxast",
        "react",
        "repl"
    ],
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "import": "./lib/mdx-live.modern.js",
            "browser": "./lib/mdx-live.modern.js"
        },
        "./package.json": "./package.json"
    },
    "repository": "git@github.com:bloczjs/mdx.git",
    "author": "Ayc0 <ayc0.benj@gmail.com>",
    "license": "MIT",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rm -rf lib/ && microbundle -f modern,esm",
        "test": "ava"
    },
    "dependencies": {
        "@types/mdx": "^2.0.3",
        "unist-util-select": "^4.0.0"
    },
    "peerDependencies": {
        "@mdx-js/mdx": "^2.0.0",
        "react": ">=18.0.0"
    },
    "devDependencies": {
        "@happy-dom/global-registrator": "^18.0.1",
        "@mdx-js/mdx": "^2.1.2",
        "@mdx-js/react": "^2.2.1",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/react": "^16.3.0",
        "@types/estree": "^0.0.52",
        "@types/react": "19.1.8",
        "ava": "^6.4.1",
        "microbundle": "^0.15.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "ts-node": "^10.9.1",
        "vfile": "^5.3.4"
    },
    "ava": {
        "extensions": {
            "ts": "module",
            "tsx": "module",
            "mjs": true,
            "cjs": true
        },
        "nodeArguments": [
            "--loader=ts-node/esm/transpile-only"
        ]
    }
}
