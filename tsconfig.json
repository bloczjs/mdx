{
    "compilerOptions": {
        "allowJs": false,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noEmit": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true
    }
}
